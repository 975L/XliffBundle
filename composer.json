{
    "name": "c975l/xliff-bundle",
    "type": "symfony-bundle",
    "description": "This bundle exports the content of a database table in xlf files",
    "keywords": ["xliff", "xlf", "translation", "symfony", "bundle", "twig"],
    "homepage": "https://github.com/975L/XliffBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Laurent Marquet",
            "email": "laurent.marquet@laposte.net",
            "homepage": "https://github.com/Laurent3170",
            "role": "Developer"
        },
        {
            "name": "975L Community",
            "homepage": "https://github.com/975L"
        }
    ],
    "require": {
        "php": "^7",
        "doctrine/doctrine-bundle": "^1",
        "doctrine/orm": "^2",
        "sensio/framework-extra-bundle": "*",
        "symfony/framework-bundle": "*",
        "twig/twig": "^2"
    },
    "require-dev": {
        "phpstan/phpstan": "^0",
        "phpunit/phpunit": "^7",
        "rector/rector": "^0",
        "symplify/easy-coding-standard": "^5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "c975L\\XliffBundle\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "support": {
        "email": "contact@975l.com",
        "source": "https://github.com/975L/XliffBundle",
        "issues": "https://github.com/975L/XliffBundle/issues"
    }
}
